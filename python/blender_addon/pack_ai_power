#! /bin/sh

help()
{
	echo "Help ..."
}

test1()
{
	echo "Test1 ... $1 $2 $3"
}

test2()
{
	echo "Test2 ... $1 $2 $3"
}

if [ "$*" == "" ] ;
then
	help
else
	eval "$*"
fi

